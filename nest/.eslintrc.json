{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-typescript/base",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node":true
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": [
      "error", {
        "endOfLine": "auto"
      }
    ],
    "class-methods-use-this": "off",
    "no-useless-constructor": "off",
    "import/no-unresolved": "off"

  },
  "ignorePatterns": ["./dist"]
}
