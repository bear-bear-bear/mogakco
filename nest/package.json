  "scripts": {
    "prepare": "cd ../ && npx husky install nest/.husky",
    "prebuild": "rimraf dist",
    "build": "nest build && tsc-alias",
    "build-run": "node dist/main",
    "pm2-run": "pm2 start dist/main.js",
    "dev": "nest start --watch",
    "nest:debug": "nest start --debug --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:test-nest": "eslint test/**/*.ts",
    "test:e2e": "jest -i --config jest.config.e2e.ts",
    "test:unit": "jest --config jest.config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:ci": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate:create": "npm run typeorm migration:create -- -n",
    "migrate:generate": "npm run typeorm migration:generate -- -n",
    "migrate:revert": "npm run typeorm migration:revert",
    "migrate": "npm run typeorm migration:run",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "npm run schema:drop && npm run schema:sync && ts-node --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "npm run typeorm schema:drop",
    "schema:sync": "npm run typeorm schema:sync"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint . --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
