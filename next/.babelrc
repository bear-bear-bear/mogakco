{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    ["module-resolver", {
      "@assets": "./public/assets",
      "@components": "./components",
      "@hooks": "./hooks",
      "@lib": "./lib",
      "@pages":  "./pages",
      "@public":  "./public",
      "@redux": "./redux",
      "@test": "./test"
    }]
  ]
}