{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowJs": false,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": false,
    "noEmit": false,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules",
    "pages",
    "components",
    "container",
    "lib",
    "reducers",
    "selectors",
    "store",
    "styles",
    "dist"
  ],
  "include": []
}
